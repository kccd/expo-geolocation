{
  "expo": {
    "name": "expo-geolocation-example",
    "slug": "expo-geolocation-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.geolocation.example",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location permission to provide relevant functions",
        "NSLocationAlwaysUsageDescription": "We need your location permission to provide relevant functions",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location permission to provide relevant functions"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "expo.modules.geolocation.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
